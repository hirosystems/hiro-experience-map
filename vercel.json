{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/build/manifest.json",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate",
        "content-type": "application/json"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "/build/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000",
        "content-type": "image/x-icon"
      }
    },
    {
      "src": "/asset-manifest.json",
      "dest": "/build/asset-manifest.json",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate",
        "content-type": "application/json"
      }
    },
    {
      "src": "/(.*)\\.js$",
      "dest": "/build/$1.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      }
    },
    {
      "src": "/(.*)\\.css$",
      "dest": "/build/$1.css",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "text/css"
      }
    },
    {
      "src": "/(.*)\\.(png|jpg|jpeg|gif|ico|svg)$",
      "dest": "/build/$1.$2",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)\\.(woff|woff2|ttf|eot)$",
      "dest": "/build/$1.$2",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/font-$2"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate",
        "content-type": "text/html"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
