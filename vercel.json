{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/build/manifest.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "/build/favicon.ico",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/asset-manifest.json",
      "dest": "/build/asset-manifest.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)\\.js$",
      "dest": "/build/$1.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "text/html"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
